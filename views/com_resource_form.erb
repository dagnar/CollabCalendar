<script>
	$(document).ready(function(){
		var valArr = new Array();

		$("#button").click(function() {
			valArr[0] = $("#name").val();
			valArr[1] = $("#description").val();
			valArr[2] = $("#location").val();
			valArr[3] = $("#bookable").val();

			$.post("",
						{
							crId: valArr[0],
							name: valArr[1],
							startDate: valArr[2],
							startTime: valArr[3],
							endTime: valArr[4]
						},
						function(data, status){
							 alert("Data: " + data + "\nStatus: " + status);
						}
				);
		});		
	});
</script>

<form action="/common-resource/new" method="post">
	<p>Name: <input type="text" name="name" size="20"/></p>
	<p>Description: <input type="text" name="description" size="20"/></p>
	<p>Location: <input type="text" name="location" size="20"/></p>
	<p>Bookable: <input type="radio" name="bookable" value="true">True
							 <input type="radio" name="bookable" value="false">False</p>
	<input type="submit" value="YEAH!">
</form>